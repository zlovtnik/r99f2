# Google Analytics 4 Configuration
# Get your Measurement ID from Google Analytics console (format: G-XXXXXXXXXX)
# This is required for analytics tracking to work
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Feature Flags (set to 'false' to disable, any other value or unset = enabled)
# Contact form functionality
VITE_ENABLE_CONTACT_FORM=true
# Service areas page and functionality
VITE_ENABLE_SERVICE_AREAS=true
# Testimonials section
VITE_ENABLE_TESTIMONIALS=true
# FAQ section
VITE_ENABLE_FAQ=true
# Analytics tracking
VITE_ENABLE_ANALYTICS=true
# Social sharing buttons
VITE_ENABLE_SOCIAL_SHARING=true
